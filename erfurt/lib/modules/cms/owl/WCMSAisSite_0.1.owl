   <bibtex:hasPublisher rdf:datatype="http://www.w3.org/2001/XMLSchema#string">IEEE</bibtex:hasPublisher>
   <bibtex:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detecting Moving Objects in Airborne Forward Looking Infra-Red Sequences</bibtex:hasTitle>
