   <bibtex:hasTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Relationship-based Clustering and Cluster Ensembles for High-dimensional Data Mining</bibtex:hasTitle>
   <bibtex:hasURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://strehl.com/download/strehl-phd.pdf</bibtex:hasURL>
   <bibtex:hasYear rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2002</bibtex:hasYear>
</rdf:Description>

