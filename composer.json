{
    "name": "aksw/erfurt",
    "description": "PHP5 / Zend based Semantic Web API for Social Semantic Software",
    "keywords": ["semantic", "sparql", "rdf"],
    "homepage": "http://erfurt-framework.org/",
    "minimum-stability": "stable",
    "include-path": [
        "library"
    ],
    "autoload": {
        "classmap": [
            "library/antlr/",
            "library/Erfurt/",
            "tests/"
        ]
    },
    "require": {
        "php": ">=5.3.3",
        "semsol/arc2": "*",
        "zendframework/zendframework1": ">=1.11.5",
        "doctrine/dbal": "~2.4.1",
        "symfony/config": "~2.3.0",
        "symfony/dependency-injection": "~2.3.0",
        "soundintheory/php-sql-parser": "~1.0.0",
        "guzzle/guzzle": "~3.7.0"
    },
    "require-dev": {
        "athletic/athletic": "dev-master",
        "fzaninotto/faker": "~1.3.0",
        "phpunit/phpunit": "4.5.*",
        "squizlabs/php_codesniffer": "*"
    },
    "suggest": {
        "ext-oci8": "To use the Oracle Triple Store."
    },
    "config": {
        "use-include-path": true
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Matthimatiker/athletic"
        }
    ]
}
