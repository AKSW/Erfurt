  <aksw:IncubatorProject rdf:about="&people;Projects/CSVImport"
    doap:programming-language="php
JavaScript"
    rdfs:label="CSVImport">
    <aksw:hookline>Representing multi-dimensional statistical data as RDF using the RDF Data Cube Vocabulary</aksw:hookline>
    <aksw:relatedProject rdf:resource="&people;Projects/CubeViz" />
    <aksw:relatedProject rdf:resource="&people;Projects/GHO" />
