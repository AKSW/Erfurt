    <doap:maintainer rdf:resource="SoerenAuer" />
    <foaf:homepage rdf:resource="http://dbpedia.org" />
    <foaf:logo rdf:resource="http://lh3.ggpht.com/-boPYCFsH6_g/TlzV0n3IoFI/AAAAAAAAApA/vLA-xusLvEU/s800/dbpedia_logo.png" />
